* @author NEIL 12345
*/


public class PrintVoltageWater {
	
	static double R;
	static final double Ro = 50;
	static final double K = 0.5;
	static double Vm;
	static final double Rs = 75;
	static final double Vs = 20;
	static double T = 10;
	
 public static void main(String[] args) {
 R = Ro + (K*T);
Vm = ( R / ( Rs + R )) * Vs;
	println(Vm); 
 }
 
}
